{
    "version": 2,
    "builds": [{
        "src": "dist/server.js",
        "use": "@vercel/node"
    }],
    "routes": [{
        "src": "/(.*)",
        "methods": ["OPTIONS"],
        "dest": "/api/cors-preflight"
    },
    {
        "src": "/(.*)",
        "dest": "dist/server.js",
        "headers": {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
            "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
        }
    }],
    "env": {
        "NODE_ENV": "production"
    }
}
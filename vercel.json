{
    "version": 2,
    "builds": [{
        "src": "dist/server.js",
        "use": "@vercel/node"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "dist/server.js"
    }],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "dist/server.js": {
            "memory": 1024,
            "maxDuration": 60
        }
    }
}

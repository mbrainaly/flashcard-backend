{
    "version": 2,
    "builds": [{
        "src": "dist/server.js",
        "use": "@vercel/node"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "dist/server.js",
        "headers": {
            "Access-Control-Allow-Origin": "https://aiflashcard.net",
            "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
            "Access-Control-Allow-Headers": "Content-Type,Authorization",
            "Access-Control-Allow-Credentials": "true"
        }
    }],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "dist/server.js": {
            "memory": 1024,
            "maxDuration": 60
        }
    }
}